<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.gdou.gym_java.mapper.RentEquipmentMapper">

    <select id="queryRentEquipment" resultType="edu.gdou.gym_java.entity.model.RentEquipment">
        select * from Rent_equipment
        <where>
            <if test="rid!='' and rid!=null">rid = #{rid}</if>
            <if test="eid!='' and eid!=null">eid = #{eid}</if>
            <if test="eName!='' and eName!=null">eName = #{eName}</if>
            <if test="uid!='' and uid!=null">uid = #{uid}</if>
            <if test="username!='' and username!=null">username = #{username}</if>
            <if test="rentTime!='' and rentTime!=null">rentTime = #{rentTime}</if>
            <if test="number!='' and number!=null">number = #{number}</if>
        </where>
    </select>

</mapper>